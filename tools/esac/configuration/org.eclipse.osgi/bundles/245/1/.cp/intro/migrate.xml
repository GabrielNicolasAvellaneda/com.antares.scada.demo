<?xml version="1.0" encoding="UTF-8" ?>
<introContent>

  <extensionContent id="org.eclipse.scada.hmi.admin.client.app" name="Extra"
    alt-style="css/swt.properties" style="css/migrate.css"
    path="migrate/@">
    <group style-id="content-group" id="content-group">
      <link label="From openSCADA"
        url="http://org.eclipse.ui.intro/showPage?id=migrate-openscada"
        style-id="content-link" id="extra-openscada">
        <text>Migrating from openSCADA</text>
      </link>
      <anchor id="additions"/>
    </group>
  </extensionContent>

<page id="migrate-openscada" style="$theme$/html/migrate.css"
    style-id="page">
    <title style-id="intro-header">$introTitle$</title>
    <group id="extra-group1" filteredFrom="swt" />
    <include path="overview/navigation-links" />
    <group id="page-content">
      <group id="content-header" label="Migrating from openSCADA" filteredFrom="swt">
      </group>
      <text style-id="page-title" id="page-title" filteredFrom="html">
        Migrating from openSCADA
      </text>
      <text style-id="page-description" id="page-description">
        This document describes what to expect when migrating from openSCADA to Eclipse SCADA.
      </text>
      <html src="html/migrate.xhtml" type="inline"></html>
    </group>
  </page>

</introContent>
